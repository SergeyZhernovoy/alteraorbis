  <!-- Creating animation
    1. Add a model resource, with the usual flags, plus: animatimation="create"
    2. Run the 'animation' test scene.
    3. Select the model, make sure the gun/effects are off. Switch to fullscreen. Switch to ortho.
    4. Export. Record the PUR
    5. Add the animation tag with the obvious values, including the PUR just used.
    6. Make the resource, should now have basic animation. Can add additional in Spriter.
  -->

  <animation filename="humanFemale/humanFemale.scml" assetName="humanFemaleAnimation" pur="235.9" >
    <meta animation="melee" name="impact" time="400.0" />
  </animation>
  <animation filename="mantis/mantis.scml" assetName="mantisAnimation" pur="340.5" >
    <meta animation="melee" name="impact" time="300.0" />
  </animation>
  <animation filename="balrog/balrog.scml" assetName="balrogAnimation" pur="339">
    <meta animation="melee" name="impact" time="800.0" />
  </animation>
 
  <!-- Tactical Sprite models are centered at (0,0,0) 
